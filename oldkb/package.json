{
    "name": "jintong-knowledge-base",
    "version": "1.0.0",
    "description": "Jintong knowledge base.",
    "scripts": {
        "gulp:dev": "gulp --gulpfile config/gulpfile.js --cwd ./",
        "gulp:p": "gulp --gulpfile config/gulpfile.js --cwd ./",
        "gulp:d": "gulp watch --gulpfile config/gulpfile.js --cwd ./",
        "webpack:dev": "webpack --config config/webpack.dev.js",
        "webpack:p": "webpack --config config/webpack.prod.js",
        "webpack:d": "webpack-dev-server --config config/webpack.dev.js ",
        "mvn": "cd ../ && mvn -D\"file.encoding\"=utf-8 spring-boot:run",
        "prod": "npm run clean && npm run webpack:p && npm run gulp:p",
        "dev": "npm run clean && npm run webpack:dev && concurrently -k -r \"npm run gulp:d\" \"npm run webpack:d\" ",
        "preclean": "npm install --registry https://registry.npm.taobao.org",
        "clean": "rimraf ./config/manifest.json ./main/resources/public/dist",
        "sass": "rimraf ./main/resources/public/modules/materialize-css/ && gulp sass --gulpfile config/gulpfile.js --cwd ./ && sass ./config/style.scss ./main/resources/public/modules/materialize-css/css/materialize.css",
        "next": "cd ../client && npm run dev",
        "tsc": "tsc --outDir ./tmp"
    },
    "author": "Jintongsoft",
    "private": true,
    "devDependencies": {
        "assets-webpack-plugin": "^3.5.0",
        "autoprefixer": "^6.6.0",
        "awesome-typescript-loader": "^3.2.2",
        "concurrently": "^3.1.0",
        "css-loader": "^0.26.1",
        "cssnano": "^3.9.1",
        "extract-text-webpack-plugin": "^2.1.0",
        "file-loader": "^0.9.0",
        "get-entries": "^3.0.0",
        "glob": "^7.1.1",
        "gulp": "^3.9.1",
        "gulp-data": "^1.2.1",
        "gulp-plumber": "^1.1.0",
        "gulp-pug": "^3.2.0",
        "less": "^2.7.1",
        "less-loader": "^2.2.3",
        "mkdirp": "^0.5.1",
        "postcss-loader": "^1.2.1",
        "pug": "^2.0.0-rc.2",
        "pug-loader": "^2.3.0",
        "rimraf": "^2.5.4",
        "script-loader": "^0.7.0",
        "style-loader": "^0.13.1",
        "ts-loader": "^1.3.3",
        "tslint": "^4.2.0",
        "typescript": "^2.1.4",
        "url-loader": "^0.5.7",
        "webpack": "^2",
        "webpack-dev-server": "^2.4.1"
    },
    "dependencies": {
        "@types/d3": "^4.7.0",
        "@types/jquery": "^3.2.11",
        "@types/jquery.datatables": "^1.10.34",
        "@types/jstree": "^3.3.34",
        "@types/lodash": "^4.14.74",
        "@types/materialize-css": "^0.97.33",
        "@types/moment": "^2.13.0",
        "@types/select2": "^4.0.31",
        "core-js": "^2.4.1",
        "d3": "^4.7.4",
        "dragula": "^3.7.2",
        "echarts": "^3.4.0",
        "fine-uploader": "^5.12.0",
        "jquery": "^3.1.1",
        "jstree": "^3.3.3",
        "lodash": "^4.17.4",
        "masonry-layout": "^4.1.1",
        "material-design-icons": "^3.0.1",
        "materialize-css": "^0.97.8",
        "moment": "^2.17.1",
        "select2": "^4.0.3",
        "video.js": "^5.17.0",
        "wangeditor": "^2.1.23"
    }
}
