    "@types/isomorphic-fetch": "0.0.34",
    "@types/lodash": "^4.14.77",
    "@types/qs": "^6.2.31",
    "@types/react": "^15.6.4",
    "@types/react-dom": "^0.14.20",
    "@types/react-redux": "^4.4.35",
    "@types/redux": "^3.6.31",
    "@types/sockjs-client": "^1.0.32",
    "antd": "^2.13.6",
    "axios": "^0.16.2",
    "babel-polyfill": "^6.23.0",
    "classnames": "^2.2.5",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "normalize.css": "^7.0.0",
    "qs": "^6.5.0",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-redux": "^5.0.2",
