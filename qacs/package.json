{
  "name": "fe",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "clean": "rimraf build ../csclient/src/main/webapp/public/build ../csserver/src/main/webapp/public/build",
    "preclean": "npm install --registry http://registry.npm.jintongsoft.cn/ --no-optional",
    "build": "webpack --config config/webpack/index.js",
    "build:prod": "cross-env NODE_ENV=production npm run build && npm run copy",
    "copy": "ncp ./build ../csclient/src/main/webapp/public/build && ncp ./build ../csserver/src/main/webapp/public/build",
    "preprod": "npm run clean",
    "prod": "npm run build:prod && npm run plugin",
    "postprod": "ncp ./node_modules/@jintong ../csclient/src/main/webapp/public/build/jintong",
    "predev": "npm run clean",
    "dev": "webpack-dev-server --config config/webpack/index.js --host 0.0.0.0",
    "build:plugin": "webpack --config config/webpack/plugin.js",
    "preplugin": "rimraf ./dist/plugin ../csclient/src/main/webapp/public/plugin",
    "plugin": "npm run build:plugin",
    "postplugin": "ncp ./dist/plugin ../csclient/src/main/webapp/public/plugin"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-import": "^1.6.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "file-loader": "^0.9.0",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-plugin": "^2.29.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "ncp": "^2.0.0",
    "nodemon": "^1.11.0",
    "postcss-loader": "^1.2.2",
    "pug": "^2.0.0-rc.2",
    "pug-loader": "^2.3.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.18.2",
    "tslint": "^4.3.1",
    "tslint-react": "^2.3.0",
    "typescript": "^2.5.3",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.9.2",
    "webpack-manifest-plugin": "^1.1.0"
